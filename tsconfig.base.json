{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "es2017",
      "es2020",
      "ESNext"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "noEmit": true,
    "pretty": true,
    "noUnusedLocals": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "baseUrl": ".",
    "paths": {
      "@food-daily/mobile/constants": [
        "libs/mobile/constants/src/index.ts"
      ],
      "@food-daily/mobile/features/authorization": [
        "libs/mobile/features/authorization/src/index.ts"
      ],
      "@food-daily/mobile/features/home": [
        "libs/mobile/features/home/src/index.ts"
      ],
      "@food-daily/mobile/features/meal": [
        "libs/mobile/features/meal/src/index.ts"
      ],
      "@food-daily/mobile/features/meal-product": [
        "libs/mobile/features/meal-product/src/index.ts"
      ],
      "@food-daily/mobile/features/product": [
        "libs/mobile/features/product/src/index.ts"
      ],
      "@food-daily/mobile/features/profile": [
        "libs/mobile/features/profile/src/index.ts"
      ],
      "@food-daily/mobile/features/search-product": [
        "libs/mobile/features/search-product/src/index.ts"
      ],
      "@food-daily/mobile/features/statistics": [
        "libs/mobile/features/statistics/src/index.ts"
      ],
      "@food-daily/mobile/features/user-product": [
        "libs/mobile/features/user-product/src/index.ts"
      ],
      "@food-daily/mobile/features/user-products": [
        "libs/mobile/features/user-products/src/index.ts"
      ],
      "@food-daily/mobile/fixtures": [
        "libs/mobile/fixtures/src/index.ts"
      ],
      "@food-daily/mobile/helpers": [
        "libs/mobile/helpers/src/index.ts"
      ],
      "@food-daily/mobile/types": [
        "libs/mobile/types/src/index.ts"
      ],
      "@food-daily/mobile/ui": [
        "libs/mobile/ui/src/index.ts"
      ],
      "@food-daily/shared/types": [
        "libs/shared/types/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
